{
  "name": "@openhps/rdf",
  "version": "0.1.2",
  "description": "Open Hybrid Positioning System",
  "author": "Maxim Van de Wynckel",
  "license": "Apache-2.0",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenHPS/openhps-rdf.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./vocab": {
      "require": "./dist/cjs/vocab/index.js",
      "import": "./dist/esm/vocab/index.js",
      "types": "./dist/types/vocab/index.d.ts"
    }
  },
  "unpkg": "dist/web/openhps-rdf.min.js",
  "jsdelivr": "dist/web/openhps-rdf.min.js",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "scripts": {
    "test": "mocha --config test/.mocharc.json",
    "cover": "nyc mocha --config test/.mocharc.json && nyc report --reporter=cobertura",
    "lint": "eslint src/ --ignore-pattern vocab --ignore-pattern bin",
    "clean": "shx rm -rf ./dist && shx rm -rf ./docs/out",
    "build": "npm run clean && npm run build:declarations && npm-run-all --parallel build:esm build:cjs build:typedoc && npm run build:webpack",
    "build:declarations": "ts-node ./src/bin/generate.ts && tsc --declaration --emitDeclarationOnly --outdir dist/types",
    "build:esm": "tsc --module es2015 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs  --outDir dist/cjs",
    "build:webpack": "npm-run-all --parallel build:webpack:*",
    "build:webpack:prod": "webpack --env prod --progress --",
    "build:webpack:dev": "webpack --env dev --progress --",
    "build:typedoc": "typedoc --options docs/typedoc.json",
    "bump:development": "standard-version --prerelease alpha -m 'chore(alpha): %s' --skip.changelog",
    "bump:beta": "standard-version --prerelease beta -m 'chore(beta): %s' --skip.changelog",
    "bump:release": "standard-version -m 'chore(release): %s' --skip.changelog"
  },
  "keywords": [
    "openhps",
    "rdf"
  ],
  "dependencies": {
    "@inrupt/solid-client": "^1.14.0",
    "n3": ">=0.4.1",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "@openhps/core": ">=0.3.2",
    "reflect-metadata": ">=0.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@openhps/core": "^0.3.7-alpha.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.1",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "buffer": "^6.0.3",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "mocha": "^9.1.3",
    "mocha-junit-reporter": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "reflect-metadata": ">=0.1.13",
    "shx": "^0.3.3",
    "standard-version": "^9.3.2",
    "terser-webpack-plugin": "^5.2.4",
    "tripledoc": "^1.1.0",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.6",
    "typescript": "^4.5.0-beta",
    "webpack": "^5.59.0",
    "webpack-cli": "^4.9.1"
  },
  "nyc": {
    "branches": 0,
    "lines": 20,
    "functions": 17,
    "statements": 20,
    "include": [
      "src"
    ],
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "report-dir": "./coverage"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
